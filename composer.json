{
  "name": "devdevops/mon-projet",
  "require-dev": {
    "phpunit/phpunit": "^10",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "DevDevops\\": "src/"
    }
  },
  "scripts": {
  "lint": "vendor/bin/php-cs-fixer fix --dry-run --diff src tests",
  "analyse": "phpstan analyse src tests",
  "test": "phpunit --coverage-html build/coverage",
  "check": [
    "@lint",
    "@analyse",
    "@test"
  ]
}

}
